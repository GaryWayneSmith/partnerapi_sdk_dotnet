<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Price xmlns="http://walmart.com/">
  <itemIdentifier>
    <sku>{{sku}}</sku>
  </itemIdentifier>
  <pricingList>
    <pricing effectiveDate="{{effectiveDate}}" expirationDate="{{expirationDate}}" processMode="UPSERT">
      <currentPrice>
        <value amount="{{price}}"/>
      </currentPrice>
    </pricing>
  </pricingList>
</Price>
